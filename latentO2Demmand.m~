function varargout=latentO2Demmand(varargin)
%
%
% Returns a trained NN, training performance metrics, and generated target 
% for estimation of O2 Delivery where a high value (1) represents high 02
% demands and a low value (0) represents low O2 demands
%
% varargout=latentO2Demmand(varargin)
%
% If input is empty, returns the percent of the classification that is true
% 
%Set default pararamter values
inputs={'trainData','trainComm','commorbidityNames','show'};
outputs={'net','tr','target'};
for n=1:nargin
    if(~isempty(varargin{n}))
        eval([inputs{n} '=varargin{n};'])
    end
end

%Define the following variables as being indicative of an increase in O2
%demands
trueVar={'WEIGHT_LOSS',...
    'METASTATIC_CANCER',...
    'MALE',...
    'PSYCHOSES'};

falseVar={'OBESITY',...
         'PARALYSIS',...
         'OLD_AGE',...
         'PERIPHERAL_VASCULAR'};

[net,tr,target]=latentNet(trainData,trainComm,commorbidityNames,trueVar,falseVar,show);